{
	"servers": {
		"inkeepMcp": {
			"command": "npx",
			"args": [
				"-y",
				"mcp-remote",
				"https://mcp.inkeep.com/zod/mcp"
			],
			"type": "stdio"
		},
		"jsrepo": {
			"command": "npx",
			"args": [
				"jsrepo",
				"mcp"
			],
			"type": "stdio"
		},
		"svelte": {
			"type": "stdio",
			"command": "npx",
			"args": [
				"-y",
				"@sveltejs/mcp"
			]
		}
	},
	"inputs": []
}